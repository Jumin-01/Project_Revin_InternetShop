<Page x:Class="Project_Revin_InternetShop.Pages.AdminPages.AdminViewPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Project_Revin_InternetShop.Pages.AdminPages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="AdminViewPage">

    <Grid Background="#FF272727" >
        <ScrollViewer VerticalScrollBarVisibility="Auto" HorizontalScrollBarVisibility="Auto">
            <StackPanel x:Name="AdminCategorySP" >

            </StackPanel>
        </ScrollViewer>
        <Button x:Name="AddGameButton" Content="Add" Height="50" Width="100" VerticalAlignment="Top" HorizontalAlignment="Right" Margin="0,10,40,0" FontSize="16" Click="AddGameButton_Click" Style="{StaticResource ButtonStyle}"></Button>


    </Grid>
</Page>

